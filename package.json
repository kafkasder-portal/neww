{
  "name": "dernek-panel-ui",
  "version": "1.0.0",
  "type": "module",
  "description": "Dernek YÃ¶netim Paneli - Modern NGO Management System",
  "scripts": {
    "dev": "powershell -ExecutionPolicy Bypass -File \"./scripts/terminal-optimization.ps1\" && concurrently --kill-others-on-fail --prefix-colors \"blue,green\" \"npm run client:dev\" \"npm run server:dev\"",
    "dev:fast": "concurrently --kill-others-on-fail --prefix-colors \"blue,green\" \"npm run client:dev\" \"npm run server:dev\"",
    "client:dev": "vite --host --port 5173",
    "server:dev": "cd api && npm run dev",
    "dev:safe": "npm run cleanup && npm run dev:fast",
    "cleanup": "powershell -ExecutionPolicy Bypass -File \"./scripts/fix-terminal-hanging.ps1\"",
    "fix:css": "node scripts/fix-css-mime.js",
    "dev:css-fix": "npm run fix:css && npm run client:dev",
    "ai:optimize": "powershell -ExecutionPolicy Bypass -File \"./scripts/ai-performance-optimizer.ps1\"",
    "ai:ultimate": "powershell -ExecutionPolicy Bypass -File \"./scripts/ultimate-performance-optimizer.ps1\"",
    "cursor:ultimate": "powershell -ExecutionPolicy Bypass -File \"./scripts/ultimate-cursor-optimizer.ps1\"",
    "dev:ai": "npm run ai:optimize && npm run dev:safe",
    "dev:ultimate": "npm run ai:ultimate && npm run dev:safe",
    "dev:cursor": "npm run cursor:optimize && npm run dev:safe",
    "cursor:optimize": "powershell -ExecutionPolicy Bypass -File \"./scripts/simple-cursor-optimize.ps1\"",
    "cursor:quick": "powershell -ExecutionPolicy Bypass -File \"./scripts/cursor-quick-start.ps1\"",
    "ai:speedup": "powershell -ExecutionPolicy Bypass -File \"./scripts/ai-agent-speedup.ps1\"",
    "ai:agent": "npm run ai:speedup && npm run cursor:optimize",
    "terminal:optimize": "powershell -ExecutionPolicy Bypass -File \"./scripts/terminal-optimization.ps1\"",
    "dev:optimized": "npm run terminal:optimize && npm run dev:fast",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --report-unused-disable-directives",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "lint:quick-fix": "node scripts/fix-eslint.js",
    "lint:auto": "eslint . --fix && npm run lint:quick-fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:components": "vitest --run src/components",
    "test:hooks": "vitest --run src/hooks",
    "test:utils": "vitest --run src/utils",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test && npm run test:e2e",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "analyze:ultimate": "node scripts/ultimate-bundle-optimizer.cjs",
    "bundle:optimize": "npm run analyze:ultimate && npm run build",
    "bundle:report": "npm run analyze:ultimate && open bundle-optimization-report.json",
    "performance:analyze": "npm run build && node scripts/performance-analyze.js",
    "performance:report": "open reports/performance-report.html",
    "performance:dashboard": "powershell -ExecutionPolicy Bypass -File \"./scripts/performance-dashboard.ps1\"",
    "clean": "rm -rf dist node_modules/.vite",
    "db:setup": "cd supabase && supabase db reset",
    "setup:env": "node scripts/setup-env.js",
    "migrate:design-system": "node scripts/migrate-design-system.js",
    "migrate:corporate-ui": "node scripts/migrate-to-corporate-ui.js",
    "migrate:corporate-ui:dry": "node scripts/migrate-to-corporate-ui.js --dry-run",
    "migrate:corporate-ui:backup": "node scripts/migrate-to-corporate-ui.js --backup",
    "postinstall": "cd api && npm install"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
